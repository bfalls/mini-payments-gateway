{
  "ConnectionStrings": {
    // Default local Postgres instance (Docker)
    "Pg": "Host=localhost;Port=5432;Database=gateway;Username=postgres;Password=postgres"
  },
  "Psp": {
    // Default PSP stub URL (PspStub.Api running locally)
    "BaseUrl": "http://localhost:5279"
  },
  "Worker": {
    // How often the worker polls the outbox
    "PollIntervalSeconds": 2
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
